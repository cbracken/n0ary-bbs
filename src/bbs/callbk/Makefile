THIS_SRCDIR := $(abspath $(dir $(lastword $(MAKEFILE_LIST))))

all: CALLBK_APPS

include $(THIS_SRCDIR)/../../top.mk

CALLBK_APPS: $(CALLBK_MAKECB_APP) $(CALLBK_DISPCB_APP) $(CALLBK_SORTCB_APP)

clean: CALLBK_APP_CLEAN
install: CALLBK_MAKECB_APP_INSTALL CALLBK_DISPCB_APP_INSTALL \
	CALLBK_SORTCB_APP_INSTALL
